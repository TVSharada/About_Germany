                                    <TextBlock Text="{Binding atext}" FontSize="14" FontFamily="Times New Roman" VerticalAlignment="Center" />
                                </StackPanel>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </StackPanel>

